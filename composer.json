{
    "name": "kyaris/platform-blog",
    "type": "kyaris-extension",
    "description": "Extension for managing a simple blog",
    "authors": [
        {
            "name": "Daniel Berry",
            "email": "danielberrytn@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "http://packages.kyaris.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "5.0.*",
        "kyaris/composer-plugins": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Kyaris\\Users\\": "src/"
        }
    },
    "extra": {
        "component": "extension"
    },
    "minimum-stability": "dev"
}
